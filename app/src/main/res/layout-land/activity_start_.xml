<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@drawable/my_background1"
        tools:context=".Start_Activity">
        <TextView
                android:layout_width="140dp"
                android:layout_height="60dp" android:id="@+id/textView37"
                android:textSize="24sp" android:textAlignment="center"
                android:background="@drawable/my_button1" android:visibility="invisible"
                app:layout_constraintStart_toEndOf="@+id/editText15"
                app:layout_constraintTop_toBottomOf="@+id/button25" android:textColor="@color/silver"/>
        <TextView
                android:text="@string/conferma"
                android:layout_width="140dp"
                android:layout_height="60dp" android:id="@+id/textView47"
                android:textSize="24sp" android:textAlignment="center"
                android:background="@drawable/my_button1"
                android:visibility="invisible"
                android:layout_marginStart="8dp" app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/textView39" android:textColor="@color/silver"/>
        <TextView
                android:text="@string/sel_gen"
                android:layout_width="140dp"
                android:layout_height="60dp" android:id="@+id/textView49"
                android:textSize="24sp" android:textAlignment="center"
                android:background="@drawable/my_button1"
                android:visibility="invisible"
                app:layout_constraintStart_toEndOf="@+id/editText16"
                app:layout_constraintTop_toBottomOf="@+id/textView37"/>
        <TextView
                android:layout_width="140dp"
                android:layout_height="60dp" android:id="@+id/textView39"
                app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="8dp"
                android:textSize="24sp" android:textAlignment="center"
                android:background="@drawable/my_button1"
                app:layout_constraintTop_toBottomOf="@+id/button24" android:visibility="invisible"
                android:textColor="@color/silver"/>
        <EditText
                android:layout_width="140dp"
                android:layout_height="60dp"
                android:inputType="textPassword"
                android:text=""
                android:hint="Input..."
                android:ems="10"
                android:id="@+id/editText14"
                android:textSize="18sp"
                android:visibility="invisible"
                app:layout_constraintStart_toEndOf="@+id/textView37"
                app:layout_constraintTop_toBottomOf="@+id/button25" android:textColor="@color/sea_green"/>
        <EditText
                android:layout_width="140dp"
                android:layout_height="60dp"
                android:inputType="textPersonName"
                android:text=""
                android:hint="Input..."
                android:ems="10"
                android:id="@+id/editText16"
                android:textSize="18sp"
                android:visibility="invisible"
                app:layout_constraintStart_toEndOf="@+id/textView47"
                app:layout_constraintTop_toBottomOf="@+id/editText15" android:textColor="@color/sea_green"/>
        <EditText
                android:layout_width="140dp"
                android:layout_height="60dp"
                android:inputType="textPersonName"
                android:text=""
                android:hint="Input..."
                android:ems="10"
                android:id="@+id/editText15"
                app:layout_constraintStart_toEndOf="@+id/textView39"
                android:textSize="18sp"
                app:layout_constraintTop_toBottomOf="@+id/button25" android:visibility="invisible"
                android:textColor="@color/sea_green"/>
        <TextView
                android:text="@string/titolo_start_page"
                android:layout_width="170dp"
                android:layout_height="38dp" android:id="@+id/textView46"
                android:textSize="30sp" android:textColor="@color/gold"
                app:layout_constraintTop_toTopOf="parent"
                android:layout_marginStart="220dp" app:layout_constraintStart_toStartOf="parent"
                android:textAlignment="center"/>
        <Button
                android:text="@string/accesso"
                android:layout_width="140dp"
                android:layout_height="60dp" android:id="@+id/button24"
                android:textAllCaps="false"
                android:textSize="24sp" android:textColor="@color/silver" android:textAlignment="center"
                android:background="@drawable/my_button1"
                android:layout_marginStart="155dp"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="@+id/textView48"/>
        <Button
                android:text="@string/registrazione"
                android:layout_width="140dp"
                android:layout_height="60dp" android:id="@+id/button25"
                android:textAllCaps="false"
                android:textSize="24sp" android:textColor="@color/silver" android:textAlignment="center"
                android:background="@drawable/my_button1"
                app:layout_constraintStart_toEndOf="@+id/button24"
                app:layout_constraintTop_toTopOf="@+id/textView48"/>
        <TextView
                android:layout_width="170dp"
                android:layout_height="50dp" android:id="@+id/textView48"
                android:textAlignment="center"
                android:textSize="24sp" android:textColor="@color/silver"
                app:layout_constraintTop_toBottomOf="@+id/textView46" android:layout_marginStart="215dp"
                app:layout_constraintStart_toStartOf="parent" android:visibility="invisible"/>
        <Spinner
                android:id="@+id/spinner"
                android:layout_width="140dp"
                android:layout_height="60dp"
                android:entries="@array/maschio_femmina"
                android:visibility="invisible"
                app:layout_constraintTop_toBottomOf="@+id/editText14"
                app:layout_constraintStart_toEndOf="@+id/textView49">
        </Spinner>
        <Button
                android:layout_width="140dp"
                android:layout_height="60dp" android:id="@+id/button27"
                android:background="@drawable/my_button1" android:textSize="24sp" android:textAllCaps="false"
                android:textAlignment="center" android:visibility="invisible"
                android:text="@string/back" android:layout_marginStart="148dp"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/editText16" android:textColor="@color/red"/>
        <Button
                android:layout_width="140dp"
                android:layout_height="60dp" android:id="@+id/button28"
                android:background="@drawable/my_button1" android:textSize="24sp" android:textAllCaps="false"
                android:textAlignment="center" android:visibility="invisible"
                app:layout_constraintStart_toEndOf="@+id/button27"
                app:layout_constraintTop_toBottomOf="@+id/textView49" android:textColor="@color/smeraldo"/>
</android.support.constraint.ConstraintLayout>