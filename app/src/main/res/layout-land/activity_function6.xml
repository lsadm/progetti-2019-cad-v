<?xml version="1.0" encoding="utf-8"?>
<android.support.v4.widget.DrawerLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:id="@+id/drawer_layout"
        android:fitsSystemWindows="true"
        tools:context=".MainActivity"
        tools:openDrawer="start">
<android.support.constraint.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".Function6"
        android:background="@drawable/my_background1">
    <TextView
            android:text="@string/termine_lineare"
            android:layout_width="35dp"
            android:layout_height="40dp" android:id="@+id/textView13"
            android:textSize="24sp"
            android:background="@drawable/my_button1"
            android:textAlignment="center" android:textColor="@color/blue"
            android:layout_marginStart="8dp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textView15"/>
    <TextView
            android:text="@string/termine_quadratico"
            android:layout_width="35dp"
            android:layout_height="40dp" android:id="@+id/textView15"
            android:textSize="24sp"
            android:background="@drawable/my_button1"
            android:textAlignment="center" android:textColor="@color/blue"
            android:layout_marginStart="8dp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textView5"/>
    <Button
            android:text="@string/button2"
            android:layout_width="32dp"
            android:layout_height="40dp" android:id="@+id/somma_complessa"
            android:textSize="18sp" android:textAllCaps="false"
            android:background="@drawable/my_button1"
            android:textColor="@color/silver"
            app:layout_constraintStart_toEndOf="@+id/editText"
            app:layout_constraintTop_toBottomOf="@+id/textView5"/>
    <Button
            android:text="@string/uguale"
            android:layout_width="64dp"
            android:layout_height="40dp" android:id="@+id/calculate2"
            android:textSize="18sp" android:textAllCaps="false"
            android:background="@drawable/my_button1"
            android:textColor="@color/blue"
            app:layout_constraintStart_toEndOf="@+id/divisione_complessa"
            app:layout_constraintTop_toBottomOf="@+id/calculate7"/>
    <Button
            android:text="@string/quadrato_complesso"
            android:layout_width="32dp"
            android:layout_height="40dp" android:id="@+id/calculate6"
            android:textSize="18sp" android:textAllCaps="false"
            android:background="@drawable/my_button1"
            android:textColor="@color/silver"
            app:layout_constraintStart_toEndOf="@+id/differenza_complessa"
            app:layout_constraintTop_toTopOf="@+id/somma_complessa"/>
    <Button
            android:text="@string/radice_complessa"
            android:layout_width="32dp"
            android:layout_height="40dp" android:id="@+id/calculate7"
            android:textSize="18sp" android:textAllCaps="false"
            android:background="@drawable/my_button1"
            android:textColor="@color/silver"
            app:layout_constraintStart_toEndOf="@+id/calculate6"
            app:layout_constraintTop_toBottomOf="@+id/textView5"/>
    <Button
            android:text="@string/button3"
            android:layout_width="32dp"
            android:layout_height="40dp" android:id="@+id/differenza_complessa"
            android:textSize="18sp" android:textAllCaps="false"
            android:background="@drawable/my_button1"
            android:textColor="@color/silver"
            app:layout_constraintStart_toEndOf="@+id/somma_complessa"
            app:layout_constraintTop_toTopOf="@+id/somma_complessa"/>
    <Button
            android:text="@string/button4"
            android:layout_width="32dp"
            android:layout_height="40dp" android:id="@+id/moltiplicazione_complessa"
            android:textSize="18sp" android:textAllCaps="false"
            android:background="@drawable/my_button1"
            android:textColor="@color/silver"
            app:layout_constraintStart_toEndOf="@+id/editText2"
            app:layout_constraintTop_toBottomOf="@+id/somma_complessa"/>
    <Button
            android:text="@string/button5"
            android:layout_width="32dp"
            android:layout_height="40dp" android:id="@+id/divisione_complessa"
            android:textSize="18sp" android:textAllCaps="false"
            android:background="@drawable/my_button1"
            android:textColor="@color/silver"
            app:layout_constraintStart_toEndOf="@+id/moltiplicazione_complessa"
            app:layout_constraintTop_toBottomOf="@+id/differenza_complessa"/>
    <EditText
            android:layout_width="100dp"
            android:layout_height="40dp"
            android:inputType="numberSigned|numberDecimal"
            android:ems="10"
            android:id="@+id/editText"
            android:textSize="18sp"
            android:textColor="@color/blue"
            app:layout_constraintStart_toEndOf="@+id/textView15"
            app:layout_constraintTop_toBottomOf="@+id/textView5" android:hint="@string/here"/>
    <EditText
            android:layout_width="100dp"
            android:layout_height="40dp"
            android:inputType="numberSigned|numberDecimal"
            android:ems="10"
            android:id="@+id/editText2"
            android:textSize="18sp" android:textColor="@color/blue"
            app:layout_constraintStart_toEndOf="@+id/textView13"
            app:layout_constraintTop_toBottomOf="@+id/editText" android:hint="@string/here"/>
    <TextView
            android:text="@string/presentazione6"
            android:layout_width="263dp"
            android:layout_height="58dp" android:id="@+id/textView5"
            android:textSize="24sp"
            android:layout_marginStart="8dp" app:layout_constraintStart_toStartOf="parent"
            android:background="@drawable/my_button1"
            android:textAlignment="center" android:textColor="@color/silver" android:layout_marginTop="8dp"
            app:layout_constraintTop_toTopOf="parent"/>
    <TextView
            android:layout_width="478dp"
            android:layout_height="45dp" android:id="@+id/textView6"
            android:textSize="18sp"
            android:background="@drawable/my_button1"
            android:textAlignment="center" android:textColor="@color/blue"
            app:layout_constraintStart_toEndOf="@+id/textView10"
            app:layout_constraintTop_toBottomOf="@+id/divisione_complessa"/>
    <TextView
            android:text="@string/fase"
            android:layout_width="60dp"
            android:layout_height="34dp" android:id="@+id/textView16"
            android:textSize="18sp"
            android:background="@drawable/my_button1"
            android:textAlignment="center" android:textColor="@color/silver"
            app:layout_constraintStart_toEndOf="@+id/calculate2"
            app:layout_constraintTop_toBottomOf="@+id/textView7"/>
    <TextView
            android:layout_width="255dp"
            android:layout_height="34dp" android:id="@+id/textView17"
            android:textSize="18sp"
            android:background="@drawable/my_button1"
            android:textAlignment="center" android:textColor="@color/blue"
            app:layout_constraintStart_toEndOf="@+id/textView16"
            app:layout_constraintTop_toBottomOf="@+id/textView18"/>
    <TextView
            android:layout_width="255dp"
            android:layout_height="34dp" android:id="@+id/textView18"
            android:textSize="18sp"
            android:background="@drawable/my_button1"
            android:textAlignment="center" android:textColor="@color/blue"
            app:layout_constraintStart_toEndOf="@+id/textView7"
            app:layout_constraintTop_toBottomOf="@+id/textView19"/>
    <TextView
            android:layout_width="255dp"
            android:layout_height="35dp" android:id="@+id/textView19"
            android:textSize="18sp"
            android:background="@drawable/my_button1"
            android:textAlignment="center" android:textColor="@color/blue"
            app:layout_constraintStart_toEndOf="@+id/textView8"
            app:layout_constraintTop_toBottomOf="@+id/textView20"/>
    <TextView
            android:layout_width="255dp"
            android:layout_height="35dp" android:id="@+id/textView20"
            android:textSize="18sp"
            android:background="@drawable/my_button1"
            android:textAlignment="center" android:textColor="@color/blue"
            android:layout_marginTop="8dp"
            app:layout_constraintTop_toTopOf="parent" app:layout_constraintStart_toEndOf="@+id/textView9"
    />
    <TextView
            android:layout_width="478dp"
            android:layout_height="45dp" android:id="@+id/textView21"
            android:textSize="18sp"
            android:background="@drawable/my_button1"
            android:textAlignment="center" android:textColor="@color/blue"
            app:layout_constraintTop_toBottomOf="@+id/textView22" app:layout_constraintStart_toEndOf="@+id/textView14"
    />
    <TextView
            android:layout_width="478dp"
            android:layout_height="45dp" android:id="@+id/textView22"
            android:textSize="18sp"
            android:background="@drawable/my_button1"
            android:textAlignment="center" android:textColor="@color/blue"
            app:layout_constraintStart_toEndOf="@+id/textView12"
            app:layout_constraintTop_toBottomOf="@+id/textView6"/>
    <TextView
            android:text="@string/modulo"
            android:layout_width="60dp"
            android:layout_height="34dp" android:id="@+id/textView7"
            android:textSize="18sp"
            android:background="@drawable/my_button1"
            android:textAlignment="center" android:textColor="@color/silver"
            app:layout_constraintStart_toEndOf="@+id/calculate2"
            app:layout_constraintTop_toBottomOf="@+id/textView8"/>
    <TextView
            android:text="@string/coefficiente_parte_immaginaria"
            android:layout_width="60dp"
            android:layout_height="35dp" android:id="@+id/textView8"
            android:textSize="18sp"
            android:background="@drawable/my_button1"
            android:textAlignment="center" android:textColor="@color/silver"
            app:layout_constraintTop_toBottomOf="@+id/textView9" app:layout_constraintStart_toEndOf="@+id/calculate2"
    />
    <TextView
            android:text="@string/parte_reale"
            android:layout_width="60dp"
            android:layout_height="35dp" android:id="@+id/textView9"
            android:textSize="18sp"
            android:background="@drawable/my_button1"
            android:textAlignment="center" android:textColor="@color/silver"
            android:layout_marginTop="8dp"
            app:layout_constraintTop_toTopOf="parent" app:layout_constraintStart_toEndOf="@+id/textView5"
    />
    <TextView
            android:text="@string/forma_algebrica"
            android:layout_width="100dp"
            android:layout_height="45dp" android:id="@+id/textView10"
            android:textSize="18sp"
            android:background="@drawable/my_button1"
            android:textAlignment="center" android:textColor="@color/silver"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="8dp"
            app:layout_constraintTop_toBottomOf="@+id/editText2"/>
    <TextView
            android:text="@string/forma_esponenziale"
            android:layout_width="100dp"
            android:layout_height="45dp" android:id="@+id/textView12"
            android:textSize="18sp"
            android:background="@drawable/my_button1"
            android:textAlignment="center" android:textColor="@color/silver"
            android:layout_marginStart="8dp" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textView10"/>
    <TextView
            android:text="@string/coniugato"
            android:layout_width="100dp"
            android:layout_height="45dp" android:id="@+id/textView14"
            android:textSize="18sp"
            android:background="@drawable/my_button1"
            android:textAlignment="center" android:textColor="@color/silver"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="8dp"
            app:layout_constraintTop_toBottomOf="@+id/textView12"/>
</android.support.constraint.ConstraintLayout>
    <android.support.design.widget.NavigationView
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_gravity="start"
            android:id="@+id/nav_view"
            app:headerLayout="@layout/nav_header"
            app:menu="@menu/nav_menu"
            app:itemTextColor="@color/sea_green">
    </android.support.design.widget.NavigationView>
</android.support.v4.widget.DrawerLayout>
