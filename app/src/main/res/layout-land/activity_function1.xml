<?xml version="1.0" encoding="utf-8"?>
<android.support.v4.widget.DrawerLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:id="@+id/drawer_layout"
        android:fitsSystemWindows="true"
        tools:context=".Function1"
        tools:openDrawer="start">
<android.support.constraint.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@drawable/my_background1"
        tools:context=".Function1">
    <Button
            android:text="@string/plot"
            android:layout_width="299dp"
            android:layout_height="45dp" android:id="@+id/button17"
            android:textColor="@color/ametista" android:textAllCaps="false"
            android:textSize="30sp"
            android:background="@drawable/my_button1"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="8dp"
            app:layout_constraintTop_toBottomOf="@+id/textView30" android:visibility="invisible"/>
    <Button
            android:text="@string/view"
            android:layout_width="74dp"
            android:layout_height="47dp" android:id="@+id/button20"
            android:textColor="@color/red" android:textAllCaps="false"
            android:textSize="18sp"
            android:background="@drawable/my_button1"
            android:layout_marginStart="8dp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/button30" android:layout_marginTop="32dp"
            android:visibility="invisible"/>
    <TextView
            android:layout_width="100dp"
            android:layout_height="38dp" android:id="@+id/textView26"
            android:textAppearance="@style/TextAppearance.AppCompat"
            android:textColor="@color/silver"
            android:textSize="14sp" android:textAllCaps="false"
            android:background="@drawable/my_button1"
            android:textAlignment="center"
            app:layout_constraintStart_toEndOf="@+id/button11"
            app:layout_constraintTop_toBottomOf="@+id/textView29" android:layout_marginStart="32dp"
            android:layout_marginTop="28dp" android:visibility="invisible"/>
    <Button
            android:text="@string/poligono"
            android:layout_width="99dp"
            android:layout_height="43dp"
            android:id="@+id/button8"
            android:textColor="@color/silver"
            android:textAllCaps="false"
            android:textSize="14sp"
            android:background="@drawable/my_button1"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="8dp"
            app:layout_constraintTop_toBottomOf="@+id/pres"/>
    <Button
            android:text="@string/retta"
            android:layout_width="100dp"
            android:layout_height="43dp"
            android:id="@+id/button9"
            android:textColor="@color/silver"
            android:textAllCaps="false"
            android:textSize="14sp" android:background="@drawable/my_button1"
            app:layout_constraintStart_toEndOf="@+id/button8"
            app:layout_constraintTop_toBottomOf="@+id/pres"/>
    <Button
            android:text="@string/circonferenza"
            android:layout_width="100dp"
            android:layout_height="43dp" android:id="@+id/button10"
            android:textColor="@color/silver" android:textAllCaps="false"
            android:textSize="14sp"
            android:background="@drawable/my_button1"
            app:layout_constraintStart_toEndOf="@+id/button9"
            app:layout_constraintTop_toBottomOf="@+id/pres"/>
    <Button
            android:text="@string/iperbole"
            android:layout_width="100dp"
            android:layout_height="43dp" android:id="@+id/button11"
            android:textColor="@color/silver"
            android:textAllCaps="false"
            android:textSize="14sp"
            android:background="@drawable/my_button1"
            app:layout_constraintStart_toEndOf="@+id/button31"
            app:layout_constraintTop_toBottomOf="@+id/button10"/>
    <Button
            android:text="@string/parabola"
            android:layout_width="99dp"
            android:layout_height="43dp"
            android:id="@+id/button30" android:background="@drawable/my_button1"
            android:textAllCaps="false" android:textSize="14sp" android:textColor="@color/silver"
            android:layout_marginStart="8dp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/button8"/>
    <Button
            android:text="@string/ellisse"
            android:layout_width="100dp"
            android:layout_height="43dp"
            android:id="@+id/button31" android:background="@drawable/my_button1"
            android:textAllCaps="false" android:textSize="14sp" android:textColor="@color/silver"
            app:layout_constraintStart_toEndOf="@+id/button30"
            app:layout_constraintTop_toBottomOf="@+id/button9"/>
    <TextView
            android:layout_width="300dp"
            android:layout_height="43dp" android:id="@+id/pres"
            android:text="@string/presentazione1"
            android:textAlignment="center"
            android:textSize="24sp" android:background="@drawable/my_button1" android:textColor="@color/silver"
            android:layout_marginStart="8dp" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginTop="16dp" app:layout_constraintTop_toTopOf="parent"/>
    <EditText
            android:layout_width="130dp"
            android:layout_height="38dp"
            android:inputType="numberDecimal|numberSigned"
            android:ems="10"
            android:id="@+id/editText4"
            android:textSize="14sp" android:textColor="@color/blue"
            android:hint="@string/here"
            app:layout_constraintStart_toEndOf="@+id/textView24"
            android:layout_marginTop="28dp" app:layout_constraintTop_toTopOf="parent" android:visibility="invisible"/>
    <EditText
            android:layout_width="130dp"
            android:layout_height="38dp"
            android:inputType="numberDecimal"
            android:ems="10"
            android:id="@+id/editText6"
            android:textSize="14sp"
            android:textColor="@color/blue" android:hint="@string/here"
            app:layout_constraintStart_toEndOf="@+id/textView26"
            app:layout_constraintTop_toBottomOf="@+id/editText10" android:layout_marginTop="28dp"
            android:visibility="invisible"/>
    <TextView
            android:layout_width="100dp"
            android:layout_height="38dp" android:id="@+id/textView24"
            android:background="@drawable/my_button1"
            android:textAlignment="center" android:textColor="@color/silver" android:textSize="14sp"
            app:layout_constraintStart_toEndOf="@+id/pres"
            android:layout_marginTop="28dp" app:layout_constraintTop_toTopOf="parent"
            android:layout_marginStart="32dp" android:visibility="invisible"/>
    <TextView
            android:layout_width="100dp"
            android:layout_height="38dp" android:id="@+id/textView28"
            android:background="@drawable/my_button1" android:textAlignment="center" android:textColor="@color/silver"
            app:layout_constraintStart_toEndOf="@+id/textView30"
            app:layout_constraintTop_toBottomOf="@+id/textView26" android:layout_marginStart="32dp"
            android:layout_marginTop="28dp" android:visibility="invisible"/>
    <TextView
            android:layout_width="100dp"
            android:layout_height="38dp" android:id="@+id/textView29"
            android:background="@drawable/my_button1"
            android:textAlignment="center" android:textColor="@color/silver"
            app:layout_constraintStart_toEndOf="@+id/button10"
            app:layout_constraintTop_toBottomOf="@+id/textView24" android:layout_marginStart="32dp"
            android:layout_marginTop="28dp" android:visibility="invisible"/>
    <EditText
            android:layout_width="130dp"
            android:layout_height="38dp"
            android:inputType="numberDecimal|numberSigned"
            android:ems="10"
            android:id="@+id/editText9"
            android:textSize="14sp"
            android:textColor="@color/blue"
            android:hint="@string/here"
            app:layout_constraintStart_toEndOf="@+id/textView28"
            app:layout_constraintTop_toBottomOf="@+id/editText6" android:layout_marginTop="28dp"
            android:visibility="invisible"/>
    <EditText
            android:layout_width="130dp"
            android:layout_height="38dp"
            android:inputType="numberDecimal|numberSigned"
            android:ems="10"
            android:id="@+id/editText10"
            android:textSize="14sp"
            android:textColor="@color/blue"
            android:hint="@string/here"
            app:layout_constraintStart_toEndOf="@+id/textView29"
            app:layout_constraintTop_toBottomOf="@+id/editText4" android:layout_marginTop="28dp"
            android:visibility="invisible"/>
    <TextView
            android:layout_width="225dp"
            android:layout_height="47dp" android:id="@+id/textView30"
            android:textSize="14sp" android:textAlignment="center"
            android:textColor="@color/blue" android:background="@drawable/my_button1"
            app:layout_constraintStart_toEndOf="@+id/button20"
            app:layout_constraintTop_toBottomOf="@+id/button31" android:layout_marginTop="32dp"
            android:visibility="invisible"/>

</android.support.constraint.ConstraintLayout>
<android.support.design.widget.NavigationView
android:layout_width="match_parent"
android:layout_height="match_parent"
android:layout_gravity="start"
android:id="@+id/nav_view1"
app:headerLayout="@layout/nav_header"
app:menu="@menu/nav_menu"
app:itemTextColor="@color/sea_green">
</android.support.design.widget.NavigationView>
        </android.support.v4.widget.DrawerLayout>
