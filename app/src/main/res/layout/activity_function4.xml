<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
xmlns:android="http://schemas.android.com/apk/res/android"
xmlns:tools="http://schemas.android.com/tools"
xmlns:app="http://schemas.android.com/apk/res-auto"
android:layout_width="match_parent"
android:layout_height="match_parent"
android:background="@drawable/my_background1"
tools:context=".Function4">
    <TextView
            android:text="@string/termine_costante"
            android:layout_width="45dp"
            android:layout_height="45dp" android:id="@+id/textView11"
            android:textSize="24sp"
            android:background="@drawable/my_button1" android:textAlignment="center" android:textColor="@color/silver"
            android:layout_marginStart="8dp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textView13"/>
    <TextView
            android:text="@string/termine_lineare"
            android:layout_width="45dp"
            android:layout_height="45dp" android:id="@+id/textView13"
            android:textSize="24sp"
            android:background="@drawable/my_button1"
            android:textAlignment="center" android:textColor="@color/silver"
            android:layout_marginStart="8dp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textView15"/>
    <TextView
            android:text="@string/termine_quadratico"
            android:layout_width="45dp"
            android:layout_height="45dp" android:id="@+id/textView15"
            android:textSize="24sp"
            android:background="@drawable/my_button1"
            android:textAlignment="center" android:textColor="@color/silver"
            android:layout_marginStart="8dp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textView5"/>
    <Button
            android:text="@string/calculate"
            android:layout_width="195dp"
            android:layout_height="45dp" android:id="@+id/calculate"
            android:textSize="24sp" android:textAllCaps="false"
            android:background="@drawable/my_button1"
            android:textColor="@color/silver"
            app:layout_constraintStart_toEndOf="@+id/editText"
            app:layout_constraintTop_toBottomOf="@+id/textView5"/>
    <TextView
            android:layout_width="195dp"
            android:layout_height="90dp" android:id="@+id/textView"
            android:textSize="18sp"
            android:background="@drawable/my_button1" android:textColor="@color/blue"
            app:layout_constraintTop_toBottomOf="@+id/calculate"
            app:layout_constraintStart_toEndOf="@+id/editText2" android:textAlignment="center"/>
    <EditText
            android:layout_width="105dp"
            android:layout_height="45dp"
            android:inputType="numberSigned|numberDecimal"
            android:ems="10"
            android:id="@+id/editText"
            android:textSize="24sp"
            android:textColor="@color/blue"
            app:layout_constraintStart_toEndOf="@+id/textView15"
            app:layout_constraintTop_toBottomOf="@+id/textView5" android:hint="@string/here"/>
    <EditText
            android:layout_width="105dp"
            android:layout_height="45dp"
            android:inputType="numberSigned|numberDecimal"
            android:ems="10"
            android:id="@+id/editText2"
            android:textSize="24sp" android:textColor="@color/blue"
            app:layout_constraintTop_toBottomOf="@+id/editText" app:layout_constraintStart_toEndOf="@+id/textView13"
            android:hint="@string/here"/>
    <EditText
            android:layout_width="105dp"
            android:layout_height="45dp"
            android:inputType="numberSigned|numberDecimal"
            android:ems="10"
            android:id="@+id/editText3"
            android:textSize="24sp"
            android:textColor="@color/blue"
            app:layout_constraintTop_toBottomOf="@+id/editText2" app:layout_constraintStart_toEndOf="@+id/textView11"
            android:hint="@string/here"/>
    <TextView
            android:text="@string/presentazione4"
            android:layout_width="345dp"
            android:layout_height="wrap_content" android:id="@+id/textView5"
            android:textSize="24sp"
            android:layout_marginStart="8dp" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginEnd="8dp" app:layout_constraintEnd_toEndOf="parent"
            android:background="@drawable/my_button1" app:layout_constraintHorizontal_bias="0.0"
            android:textAlignment="center" android:textColor="@color/silver" android:layout_marginTop="16dp"
            app:layout_constraintTop_toTopOf="parent"/>
</android.support.constraint.ConstraintLayout>